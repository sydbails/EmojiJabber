<h1> Specific Room </h1>

<%= @room.name %>

    <%= form_for :favorite, :url => url_for(controller: "favorites", action: "create") do |f|%>
    <%= f.hidden_field :user_id, :value => session[:user_id] %>
    <%= f.hidden_field :room_id, :value => @room.id %>
    <%= f.submit "Favorite" %> 

    <% end %>